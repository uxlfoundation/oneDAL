;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;; DAAL THREADING LAYER DESELECTIONS ;;;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
; deselect threading layer functions

[suppress_function]
symbol_name = _daal_threader_for_int64
drop = yes

;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;; DAAL DESELECTIONS ;;;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;
; deselect daal::internal namespaces

[suppress_type]
symbol_name_regexp = _ZN4daal[[:digit:]].*8internal[[:digit:]].*
drop = yes

[suppress_variable]
symbol_name_regexp = _ZN4daal[[:digit:]].*8internal[[:digit:]].*
drop = yes

[suppress_function]
symbol_name_regexp = _ZN4daal[[:digit:]].*8internal[[:digit:]].*
drop = yes

